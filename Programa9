//Programa que apresenta o maior valor númerico caso: todos sejam distintos, apenas 2 sejam iguais, apenas 3 sejam iguais, apenas 4 sejam iguais e todos sendo iguais.

#include <stdio.h>
int main(){
    int a, b, c, d, e, aux;
  printf("Digite 5 números inteiros.\n");
  scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
  printf("Números escolhidos : %d %d %d %d %d\n\nCalculando maior valor...\n\n",a,b,c,d,e);
   
   
    //Caso todos sejam distintos
    if(a>b&&(a)>c&&(a)>d&&(a)>e){printf("%d é o maior número\n",a);}else
    if(b>a&&(b)>c&&(b)>d&&(b)>e){printf("%d é o maior número\n",b);}else
    if(c>a&&(c)>b&&(c)>d&&(c)>e){printf("%d é o maior número\n",c);}else
    if(d>a&&(d)>b&&(d)>c&&(d)>e){printf("%d é o maior número\n",d);}else
    if(e>a&&(e)>b&&(e)>c&&(e)>d){printf("%d é o maior número\n",e);}else
    
    
    //Caso 2 sejam iguais
    if((a,b)>c&&(a,b)>d&&(a,b)>e&&(a=b)){printf("%d é o maior número\n",a);}else
    if((a,c)>b&&(a,b)>d&&(a,b)>e&&(a=c)){printf("%d é o maior número\n",a);}else
    if((a,d)>b&&(a,b)>c&&(a,b)>e&&(a=d)){printf("%d é o maior número\n",a);}else
    if((a,e)>b&&(a,b)>c&&(a,b)>d&&(a=e)){printf("%d é o maior número\n",a);}else
    
        if((b,c)>a&&(b,c)>d&&(b,c)>e&&(b=c)){printf("%d é o maior número\n",b);}else
        if((b,d)>a&&(b,d)>c&&(b,d)>e&&(b=d)){printf("%d é o maior número\n",b);}else
        if((b,e)>a&&(b,e)>c&&(b,e)>d&&(b=e)){printf("%d é o maior número\n",b);}else
    
    if((c,d)>a&&(c,d)>b&&(c,d)>e&&(c=d)){printf("%d é o maior número\n",c);}else
    if((c,e)>a&&(c,e)>b&&(c,e)>d&&(c=e)){printf("%d é o maior número\n",c);}else
    
        if((d,e)>a&&(d,e)>b&&(d,e)>c&&(d=e)){printf("%d é o maior número\n",d);}else
        
    //Caso 3 sejam iguais
    if((a,b,c)>d&&(a,b,c)>e&&(a=b)&&(a=c)){printf("%d é o maior número\n",a);}else
    if((a,b,d)>c&&(a,b,d)>e&&(a=b)&&(a=d)){printf("%d é o maior número\n",a);}else
    if((a,b,e)>c&&(a,b,e)>d&&(a=b)&&(a=e)){printf("%d é o maior número\n",a);}else
    if((a,c,d)>b&&(a,c,d)>e&&(a=c)&&(a=d)){printf("%d é o maior número\n",a);}else
    if((a,c,e)>b&&(a,c,e)>d&&(a=c)&&(a=e)){printf("%d é o maior número\n",a);}else
    if((a,d,e)>b&&(a,d,e)>c&&(a=d)&&(a=e)){printf("%d é o maior número\n",a);}else
    
       if((b,c,d)>a&&(b,c,d)>e&&(b=c)&&(b=d)){printf("%d é o maior número\n",b);}else
       if((b,c,e)>a&&(b,c,e)>d&&(b=c)&&(b=e)){printf("%d é o maior número\n",b);}else
       if((b,d,e)>a&&(b,d,e)>c&&(b=d)&&(b=e)){printf("%d é o maior número\n",b);}else
       
    if((c,d,e)>a&&(c,d,e)>b){printf("%d é o maior número\n",c);}else
    
    //Caso 4 sejam iguais
    if((a,b,c,d)>e&&(a=b)&&(a=c)&&(a=d)){printf("%d é o maior número\n",a,b,c,d);}else
    if((a,b,c,e)>d&&(a=b)&&(a=c)&&(a=e)){printf("%d é o maior número\n",a);}else
    if((a,c,d,e)>b&&(a=c)&&(a=d)&&(a=e)&&(a=d)){printf("%d é o maior número\n",a);}else
    if((b,c,d,e)>a&&(b=c)&&(b=d)&&(a=e)){printf("%d é o maior número\n",b);}else
    if((b,e,d,a)>c&&(b=a)&&(b=d)&&(a=e)){printf("%d é o maior número\n",b);}else
    
    //Caso todos sejam iguais
    if((a=b)&&(a=c)&&(a=d)&&(a=e)){printf("Como todos os valores são iguais, %d é o maior número.",a);}
    
        
    return 0;
}
